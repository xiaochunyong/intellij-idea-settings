<application>
  <component name="RemoteServers">
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
      </configuration>
    </remote-server>
    <remote-server name="NAS Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="tcp://192.168.3.104:2376" />
        <option name="certificatesPath" value="$USER_HOME$/.docker" />
        <option name="customConfiguratorId" value="DockerTcpConnectionConfigurator" />
      </configuration>
    </remote-server>
  </component>
</application>